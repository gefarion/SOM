ColumnarFieldSemanticsMO = LayoutMO (
	read: anIndex = (
		^(ColumnarFieldSemanticsMO columnarDataFor: self) getDataFor: self at: anIndex.
	)
	
	write: anIndex value: aValue = (
		^(ColumnarFieldSemanticsMO columnarDataFor: self) setDataFor: self at: anIndex value: aValue.
	)
	
	-------------------------------
	
	columnarDataFor: anInstance = (
		^ColumnarClasses current at: anInstance class
	)
)